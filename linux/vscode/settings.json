{
    "editor.cursorSmoothCaretAnimation": true,
    "terminal.integrated.smoothScrolling": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs":"active",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "930c70c8-474e-4e2d-a223-d1e2b9da4fb1",
    "vscord.status.button.enabled": true,
    "vscord.app.name": "Custom",
    "vscord.status.details.text.debugging": "😻 Debugging {file_name}",
    "vscord.status.details.text.editing": "😻 In {workspace} {problems}",
    "vscord.status.details.text.idle": "😻 Idling",
    "vscord.status.details.text.viewing": "😻 In {workspace} {problems}",
    "vscord.status.state.text.debugging": "😻 Debugging: {workspace}",
    "vscord.status.state.text.viewing": "😻 Viewing {file_name}",
    "vscord.status.state.text.noWorkspaceFound": "😻 No workspace.",
    "vscord.status.state.text.editing": "😻 Working on {file_name}:{current_line}:{current_column}",
    "vscord.status.image.small.idle.text": "><",
    "vscord.status.image.small.editing.text": "Nekode",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "doki.statusbar.name": "OwO",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
}
